<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.inz.uniqueidentificationsystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".otp" />
        <activity android:name=".Adhar" />
        <activity
            android:name=".Splashscreen"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="UIN"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".License"
            android:label="@string/title_activity_license"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".pancard"
            android:label="@string/title_activity_pancard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ipget" />
        <activity android:name=".Id" />
        <activity android:name=".id_back" />
        <activity android:name=".appdrawer"></activity>
    </application>

</manifest>